<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="etStyle">
        <item name="android:background">@drawable/bg_et</item>
        <item name="android:textSize">@dimen/_16sdp</item>
        <item name="android:textColorHint">@color/fog</item>
        <item name="android:textColor">@color/colorPrimary</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:inputType">text</item>
    </style>


    <style name="etStyle.Stroke">
        <item name="android:background">@drawable/bg_et_with_stroke</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>
</resources>